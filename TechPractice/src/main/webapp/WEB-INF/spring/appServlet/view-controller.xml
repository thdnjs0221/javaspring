<?xml version="1.0" encoding="UTF-8"?>
<b:beans xmlns="http://www.springframework.org/schema/mvc"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:b="http://www.springframework.org/schema/beans"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.3.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<view-controller path="/login" view-name="loginForm"/>
	
	<view-controller path="/etc/procedural" view-name="etc/proceduralView"/>
	
	<view-controller path="/etc/calculate" view-name="etc/calculateForm"/>
	
	<view-controller path="/java8/newAPI" view-name="java8/newAPI"/>
	
	<view-controller path="/rest" view-name="rest/singleView"/>

